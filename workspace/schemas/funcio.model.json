{
  "$schema": "https://raw.githubusercontent.com/NarcisOriol/codearchitect_3/bb3542a490d833055f4b86ddf22a5bf56e4f75a6/resources/codearchitect.schema.json",
  "type": "object",
  "title": "Funcio",
  "vscode-icon": "symbol-property",
  "modelType": "parent-object",
  "properties": {
    "$id": {
      "type": "string",
      "modelType": "hidden"
    },
    "$label": {
      "type": "string",
      "modelType": "hidden"
    },
    "visibility": {
      "modelType": "dropdown-select",
      "$ref": "./visibility.schema.json"
    },
    "returntype": {
      "modelType": "sub-object",
      "type":"object",
      "properties": {
        "datatype": {
          "modelType": "dropdown-select-tag",
          "type": "string",
          "const": ["types"]
        },
        "isPointer": {
          "modelType": "checkbox",
          "type":"boolean"
        },
        "isConst": {
          "modelType": "checkbox",
          "type": "boolean"
        }
      }
    },
    "parameters": {
      "modelType": "folder",
      "type": "array",
      "items": {
        "$ref": "./parameter.schema.json"
      }
    },
    "$link": {
      "modelType": "hidden",
      "type": "object",
      "properties": {
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "const": ["*", "funcions"]
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "scope": {
          "$ref": "./scope.schema.json",
          "const": "parent"
        }
      }
    }
  }
}