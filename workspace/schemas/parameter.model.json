{
  "$schema": "file:///C:/Users/narcis.oriol/Documents/codearchitect/codearchitect_3/resources/codearchitect.schema.json",
  "type": "object",
  "title": "Parameter",
  "vscode-icon": "symbol-parameter",
  "modelType": "parent-object",
  "properties": {
    "$id": {
      "type": "string",
      "hidden": true
    },
    "$label": {
      "type": "string",
      "hidden": true
    },
    "visibility": {
      "hidden": true,
      "$ref": "./visibility.schema.json",
      "const": "public"
    },
    "datatype": {
      "modelType": "dropdown-select-tag",
      "type": "string",
      "const": ["types"]
    },
    "isArray": {
      "modelType": "input-string",
      "type": "string"
    },
    "isPointer": {
      "modelType": "checkbox",
      "type": "boolean"
    },
    "pointerDirection": {
      "modelType": "dropdown-select",
      "type": "string",
      "enum": ["in", "out", "in/out"]
    },
    "isPointerToConst": {
      "modelType": "checkbox",
      "type": "boolean"
    },
    "isConstPointer": {
      "modelType": "checkbox",
      "type": "boolean"
    }
  },
  "$link": {
    "scope": "none"
  }
}
