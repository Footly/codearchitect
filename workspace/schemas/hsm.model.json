{
  "$schema": "https://raw.githubusercontent.com/NarcisOriol/codearchitect_3/bb3542a490d833055f4b86ddf22a5bf56e4f75a6/resources/codearchitect.schema.json",
  "type": "object",
  "title": "HSM",
  "modelType": "parent-object",
  "vscode-icon": "circuit-board",
  "properties": {
    "$id": {
      "type": "string",
      "modelType": "hidden"
    },
    "$label": {
      "type": "string",
      "modelType": "hidden"
    },
    "visibility": {
      "modelType": "hidden",
      "$ref": "./visibility.schema.json",
      "const": "public"
      
    },
    "events": {
      "type": "array",
      "modelType": "folder",
      "items": {
        "$ref": "./event.schema.json"
      }
    },
    "states": {
      "type": "array",
      "modelType": "folder",
      "items": {
        "$ref": "./state.schema.json"
      }
    },
    "activities": {
      "type": "array",
      "modelType": "folder",
      "items": {
        "$ref": "./activity.schema.json"
      }
    },
    "$link": {
      "modelType": "hidden",
      "type": "object",
      "properties": {
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "const": [
            "*",
            "hsm"
          ]
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "scope": {
          "$ref": "./scope.schema.json",
          "const": "local"
        }
      }
    }
  }
}