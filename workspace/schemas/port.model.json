{
  "$schema": "file:///C:/Users/narcis.oriol/Documents/codearchitect/codearchitect_3/resources/codearchitect.schema.json",
  "type": "object",
  "title": "Port",
  "properties": {
    "id": {
      "type": "string",
      "hidden": true
    },
    "label": {
      "type": "string",
      "hidden": true
    },
    "visibility": {
      "$ref": "./visibility.model.json",
      "const": "public",
      "hidden": true
    },
    "tags": {
      "hidden": true,
      "type": "array",
      "items": {
        "type": "string"
      },
      "const": [
        "port"
      ]
    },
    "icon": {
      "hidden": true,
      "type": "string",
      "const": "plug"
    },
    "from": {
      "editable": true,
      "type": "string",
      "search": {
        "query": [
          [
            {
              "field": "tags",
              "regex": "funcio"
            }
          ]
        ]
      }
    }
  }
}