{
  "$schema": "file:///C:/Users/narcis.oriol/Documents/codearchitect/codearchitect_3/resources/codearchitect.schema.json",
  "type": "object",
  "title": "Variable",
  "properties": {
    "id": {
      "type": "string",
      "hidden": true
    },
    "label": {
      "type": "string",
      "hidden": true
    },
    "visibility": {
      "$ref": "./visibility.model.json",
      "const": "public",
      "hidden": true
    },
    "tags": {
      "hidden": true,
      "type": "array",
      "items": {
        "type": "string"
      },
      "const": [
        "variable"
      ]
    },
    "icon": {
      "hidden": true,
      "type": "string",
      "const": "symbol-variable"
    },
    "datatype": {
      "hidden": true,
      "editable": true,
      "type": "string",
      "search": {
        "query": [
          [
            {
              "field": "tags",
              "regex": "types"
            }
          ]
        ]
      }
    },
    "Array": {
      "hidden": true,
      "type": "array",
      "editable": true,
      "items": {
        "type": "string",
        "search": {
          "query": [
            [
              {
                "field": "tags",
                "regex": "enummember"
              }
            ]
          ],
          "text": true
        }
      }
    },
    "isPointer": {
      "hidden": true,
      "editable": true,
      "type": "boolean"
    },
    "isVolatile": {
      "hidden": true,
      "editable": true,
      "type": "boolean"
    },
    "isConst": {
      "hidden": true,
      "editable": true,
      "type": "boolean"
    },
    "defaultValue": {
      "hidden": true,
      "editable": true,
      "type": "array",
      "items": {
        "type": "string",
        "search": {
          "query": [
            [
              {
                "field": "tags",
                "regex": "enummember"
              },
              {
                "field": "tags",
                "regex": "defines"
              }
            ]
          ],
          "text": true
        }
      }
    },
    "documentation": {
      "hidden": true,
      "editable": true,
      "multiline": true,
      "markdown": true,
      "type": "string"
    }
  }
}