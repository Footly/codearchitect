{
  "$schema": "file:///C:/Users/narcis.oriol/Documents/codearchitect/codearchitect_3/resources/codearchitect.schema.json",
  "type": "object",
  "title": "Variable",
  "vscode-icon": "symbol-variable",
  "modelType": "parent-object",
  "properties": {
    "$id": {
      "type": "string",
      "hidden": true
    },
    "$label": {
      "type": "string",
      "hidden": true
    },
    "visibility": {
      "modelType": "dropdown-select",
      "$ref": "./visibility.schema.json"
    },
    "datatype": {
      "modelType": "dropdown-select-tag",
      "type": "string",
      "const": ["types"]
    },
    "isArray": {
      "modelType": "input-string",
      "type": "string"
    },
    "isPointer": {
      "modelType": "checkbox",
      "type": "boolean"
    },
    "isVolatile": {
      "modelType": "checkbox",
      "type": "boolean"
    },
    "isConst": {
      "modelType": "checkbox",
      "type": "boolean"
    }
  },
  "$link": {
    "scope": "parent",
    "tags": ["*", "variable"]
  }
}