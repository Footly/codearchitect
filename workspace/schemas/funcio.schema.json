{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Funcio",
  "contentMediaType": "symbol-property",
  "format": "parent-object",
  "properties": {
    "$id": {
      "type": "string",
      "format": "hidden"
    },
    "$label": {
      "type": "string",
      "format": "hidden"
    },
    "visibility": {
      "format": "dropdown-select",
      "$ref": "./visibility.schema.json"
    },
    "returntype": {
      "format": "sub-object",
      "type":"object",
      "properties": {
        "datatype": {
          "format": "dropdown-select-tag",
          "type": "string",
          "const": ["types"]
        },
        "isPointer": {
          "format": "checkbox",
          "type":"boolean"
        },
        "isConst": {
          "format": "checkbox",
          "type": "boolean"
        }
      }
    },
    "parameters": {
      "format": "folder",
      "type": "array",
      "items": {
        "$ref": "./parameter.schema.json"
      }
    },
    "$link": {
      "format": "hidden",
      "type": "object",
      "properties": {
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "const": ["*", "funcions"]
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "scope": {
          "$ref": "./scope.schema.json",
          "const": "parent"
        }
      }
    }
  }
}