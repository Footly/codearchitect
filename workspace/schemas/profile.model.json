{
  "$schema": "https://raw.githubusercontent.com/NarcisOriol/codearchitect_3/bb3542a490d833055f4b86ddf22a5bf56e4f75a6/resources/codearchitect.schema.json",
  "$id": "profile.schema.json",
  "title": "Project",
  "vscode-icon": "symbol-method",
  "type": "object",
  "modelType": "root",
  "properties": {
    "$id": {
      "type": "string",
      "modelType": "hidden"
    },
    "$label": {
      "type": "string",
      "modelType": "hidden"
    },
    "$schema": {
      "type": "string",
      "modelType": "hidden"
    },
    "version": {
      "modelType": "input-string",
      "type": "string"
    },
    "documentation": {
      "modelType": "text-area",
      "type": "string"
    },
    "Classes": {
      "modelType": "array-parent-objects",
      "vscode-icon": "folder",
      "type": "array",
      "items": {
        "$ref": "./class.schema.json"
      }
    },
    "Requirements": {
      "modelType": "array-parent-objects",
      "vscode-icon": "folder",
      "type": "array",
      "items": {
        "$ref": "./reqs.schema.json"
      }
    },
    "Primitive Types": {
      "modelType": "array-parent-objects",
      "vscode-icon": "folder",
      "type": "array",
      "items": {
        "$ref": "./primitivetypes.schema.json"
      }
    },
    "$link": {
      "modelType": "hidden",
      "type": "object",
      "properties": {
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "const": ["*"]
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "scope": {
          "$ref": "./scope.schema.json",
          "const": "global"
        }
      }
    },
    "$links": {
      "type": "array",
      "modelType": "hidden",
      "items": {
        "$ref": "./tags.schema.json"
      }
    }
  }
}