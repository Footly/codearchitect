{
  "$schema": "file:///C:/Users/narcis.oriol/Documents/codearchitect/codearchitect_3/resources/codearchitect.schema.json",
  "$id": "profile.model.json",
  "title": "Project",
  "type": "object",
  "root": true,
  "properties": {
    "id": {
      "type": "string",
      "hidden": true
    },
    "label": {
      "type": "string",
      "hidden": true
    },
    "visibility": {
      "$ref": "./visibility.model.json",
      "const": "public",
      "hidden": true
    },
    "tags": {
      "hidden": true,
      "type": "array",
      "items": {
        "type": "string"
      },
      "const": [
        "project"
      ]
    },
    "icon": {
      "hidden": true,
      "type": "string",
      "const": "symbol-method"
    },
    "$model": {
      "type": "string",
      "hidden": true
    },
    "version": {
      "hidden": true,
      "editable": true,
      "type": "string"
    },
    "documentation": {
      "hidden": true,
      "editable": true,
      "multiline": true,
      "markdown": true,
      "type": "string"
    },
    "srcPath": {
      "type": "string",
      "hidden": true,
      "editable": true,
      "file": "*"
    },
    "Requirements": {
      "type": "array",
      "items": {
        "$ref": "./reqs.model.json"
      },
      "properties": {
        "icon": {
          "hidden": true,
          "type": "string",
          "const": "folder"
        }
      }
    },
    "Architecture": {
      "type": "array",
      "items": {
        "$ref": "./package.model.json"
      },
      "properties": {
        "icon": {
          "hidden": true,
          "type": "string",
          "const": "folder"
        }
      }
    },
    "Integration Tests": {
      "type": "array",
      "items": {
        "$ref": "./inttest.model.json"
      },
      "properties": {
        "icon": {
          "hidden": true,
          "type": "string",
          "const": "folder"
        }
      }
    },
    "Qualification Tests": {
      "type": "array",
      "items": {
        "$ref": "./qualitest.model.json"
      },
      "properties": {
        "icon": {
          "hidden": true,
          "type": "string",
          "const": "folder"
        }
      }
    }
  }
}