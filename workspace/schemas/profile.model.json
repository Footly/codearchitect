{
  "$schema": "file:///C:/Users/narcis.oriol/Documents/codearchitect/codearchitect_3/resources/codearchitect.schema.json",
  "$id": "profile.schema.json",
  "title": "Project",
  "vscodeIcon": "symbol-method",
  "type": "object",
  "modelType": "root",
  "properties": {
    "$id": {
      "type": "string",
      "hidden": true
    },
    "$label": {
      "type": "string",
      "hidden": true
    },
    "visibility": {
      "hidden": true,
      "$ref": "./visibility.schema.json",
      "const": "public"
    },
    "$schema": {
      "type": "string",
      "hidden": true
    },
    "version": {
      "modelType": "input-string",
      "type": "string"
    },
    "documentation": {
      "modelType": "text-area",
      "type": "string"
    },
    "Classes": {
      "modelType": "folder",
      "vscodeIcon": "folder",
      "type": "array",
      "items": {
        "$ref": "./class.schema.json"
      }
    },
    "Requirements": {
      "modelType": "folder",
      "vscodeIcon": "folder",
      "type": "array",
      "items": {
        "$ref": "./reqs.schema.json"
      }
    },
    "Primitive Types": {
      "modelType": "folder",
      "vscodeIcon": "folder",
      "type": "array",
      "items": {
        "$ref": "./primitivetypes.schema.json"
      }
    },
    "$links": {
      "type": "array",
      "hidden": true,
      "items": {
        "$ref": "./tags.schema.json"
      }
    }
  },
  "$link": {
    "scope": "local",
    "tags": ["*"]
  }
}