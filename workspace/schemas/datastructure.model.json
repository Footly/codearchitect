{
  "$schema": "https://raw.githubusercontent.com/NarcisOriol/codearchitect_3/bb3542a490d833055f4b86ddf22a5bf56e4f75a6/resources/codearchitect.schema.json",
  "type": "object",
  "title": "DataStructure",
  "modelType": "parent-object",
  "vscode-icon": "symbol-struct",
  "properties": {
    "$id": {
      "type": "string",
      "modelType": "hidden"
    },
    "$label": {
      "type": "string",
      "modelType": "hidden"
    },
    "visibility": {
      "modelType": "dropdown-select",
      "$ref": "./visibility.schema.json"
    },
    "type": {
      "modelType": "dropdown-select",
      "type": "string",
      "enum": ["struct", "union"]
    },
    "members": {
      "modelType": "folder",
      "type": "array",
      "items": {
        "$ref": "./datamember.schema.json"
      }
    },
    "$link": {
      "modelType": "hidden",
      "type": "object",
      "properties": {
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "const": ["*", "types"]
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "scope": {
          "$ref": "./scope.schema.json",
          "const": "parent"
        }
      }
    }
  }
}