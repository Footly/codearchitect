    "requirements": {
      "type": "array",
      "hidden": true,
      "editable": true,
      "items": {
        "type": "string",
        "search": {
          "query": [
            [
              {
                "field": "tags",
                "regex": "reqs"
              }
            ]
          ]
        }
      }
    },
    "satisfied by": {
      "type": "array",
      "hidden": true,
      "editable": true,
      "items": {
        "type": "string",
        "search": {
          "query": [
            [
              {
                "field": "tags",
                "regex": "unittest"
              },
              {
                "field": "tags",
                "regex": "inttest"
              }
            ]
          ]
        }
