{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Transition",
    "contentMediaType": "symbol-event",
    "format": "parent-object",
    "properties": {
      "$id": {
        "type": "string",
        "format": "hidden"
      },
      "$label": {
        "type": "string",
        "format": "hidden"
      },
      "to": {
        "type": "string",
        "format": "dropdown-select-tag",
        "$ref": "./tags.schema.json",
        "const": ["state"]
      },
      "$tag": {
      "format": "hidden",
      "type": "object",
      "properties": {
        "$filter": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "const": ["*", "transition"]
        },
        "$scope": {
          "type": "string",
          "const": "global"
        }
      }
    },
    "$tags": {
      "type": "array",
      "format": "hidden",
      "items": {
        "type": "object",
        "properties": {
          "$tag": {
            "type": "array",
            "items": {
              "$ref": "./tags.schema.json"
            }
          },
          "$id": {
            "type": "string"
          }
        }
      }
    }
    }
  }