{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Transition",
  "contentMediaType": "symbol-event",
  "format": "parent-object",
  "properties": {
    "$id": {
      "type": "string",
      "format": "hidden"
    },
    "$label": {
      "type": "string",
      "format": "hidden"
    },
    "to": {
      "type": "string",
      "format": "dropdown-select-tag",
      "const": [
        "state"
      ]
    },
    "$link": {
      "format": "hidden",
      "type": "object",
      "properties": {
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "const": [
            "*",
            "transition"
          ]
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "scope": {
          "type": "string",
          "const": "parent"
        }
      }
    }
  }
}