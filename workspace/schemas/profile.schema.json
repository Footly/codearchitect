{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "profile.schema.json",
  "title": "Project",
  "contentMediaType": "symbol-method",
  "type": "object",
  "format": "root-object",
  "properties": {
    "$id": {
      "type": "string",
      "format": "hidden"
    },
    "$label": {
      "type": "string",
      "format": "hidden"
    },
    "$schema": {
      "type": "string",
      "format": "hidden"
    },
    "Classes": {
      "format": "array-parent-objects",
      "contentMediaType": "folder",
      "type": "array",
      "items": {
        "$ref": "./class.schema.json"
      }
    },
    "Requirements": {
      "format": "array-parent-objects",
      "contentMediaType": "folder",
      "type": "array",
      "items": {
        "$ref": "./reqs.schema.json"
      }
    },
    "Primitive Types": {
      "format": "array-parent-objects",
      "contentMediaType": "folder",
      "type": "array",
      "items": {
        "$ref": "./primitivetypes.schema.json"
      }
    },
    "$link": {
      "format": "hidden",
      "type": "object",
      "properties": {
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "const": ["*"]
        },
        "dependencies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "scope": {
          "$ref": "./scope.schema.json",
          "const": "global"
        }
      }
    },
    "$links": {
      "type": "array",
      "format": "hidden",
      "items": {
        "$ref": "./tags.schema.json"
      }
    }
  }
}