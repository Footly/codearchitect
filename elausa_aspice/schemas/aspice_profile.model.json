{
  "$schema": "file:///C:/Users/narci/Documents/codearchitect/codearchitect_3/resources/codearchitect.schema.json",
  "$id": "aspice_profile.model.json",
  "title": "Project",
  "type": "object",
  "root": true,
  "properties": {
    "id": {
      "type": "string",
      "hidden": true
    },
    "label": {
      "type": "string",
      "hidden": true
    },
    "tags": {
      "hidden": true,
      "type": "array",
      "items": {
        "type": "string"
      },
      "const": [
        "project"
      ]
    },
    "icon": {
      "hidden": true,
      "type": "string",
      "const": "symbol-method"
    },
    "$model": {
      "type": "string",
      "hidden": true
    },
    "description": {
      "hidden": true,
      "editable": true,
      "multiline": true,
      "type": "string"
    },
    "SWE1-Requirements": {
      "type": "array",
      "items": {
        "$ref": "./reqs.model.json"
      },
      "properties": {
        "icon": {
          "hidden": true,
          "editable": false,
          "type": "string",
          "const": "folder"
        }
      }
    },
    "SWE2-Architecture": {
      "type": "object",
      "properties": {
        "Layers": {
          "type": "array",
          "items": {
            "$ref": "./layer.model.json"
          },
          "properties": {
            "icon": {
              "hidden": true,
              "editable": false,
              "type": "string",
              "const": "folder"
            }
          },
          "editable": false
        },
        "Interfaces": {
          "type": "array",
          "items": {
            "$ref": "./interface.model.json"
          },
          "properties": {
            "icon": {
              "hidden": true,
              "editable": false,
              "type": "string",
              "const": "folder"
            }
          },
          "editable": false
        },
        "icon": {
          "hidden": true,
          "editable": false,
          "type": "string",
          "const": "folder"
        }
      },
      "editable": false
    }
  }
}